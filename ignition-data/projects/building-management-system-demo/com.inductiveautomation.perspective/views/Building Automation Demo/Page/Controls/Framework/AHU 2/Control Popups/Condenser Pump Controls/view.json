{
  "custom": {},
  "params": {},
  "props": {
    "defaultSize": {
      "height": 106,
      "width": 637
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "position": {
              "basis": "60px"
            },
            "props": {
              "style": {
                "color": "--neutral-100",
                "textAlign": "center"
              },
              "text": "HOA"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Multi State Button"
            },
            "position": {
              "basis": "30px"
            },
            "propConfig": {
              "props.controlValue": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "fallbackDelay": 2.5,
                    "mode": "direct",
                    "tagPath": "[default]BMS/Blue/HVAC/AC2/Cond_Pump/HMI_HOA"
                  },
                  "type": "tag"
                }
              },
              "props.indicatorValue": {
                "binding": {
                  "config": {
                    "path": "this.props.controlValue"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "orientation": "row",
              "states": [
                {
                  "selectedStyle": {
                    "backgroundColor": "#FFFF00",
                    "classes": "",
                    "color": "#000000"
                  },
                  "text": "Hand",
                  "unselectedStyle": {
                    "classes": ""
                  },
                  "value": 2
                },
                {
                  "selectedStyle": {
                    "backgroundColor": "#FF0000",
                    "classes": "",
                    "color": "#000000"
                  },
                  "text": "Off",
                  "unselectedStyle": {
                    "classes": ""
                  },
                  "value": 0
                },
                {
                  "selectedStyle": {
                    "backgroundColor": "#00FF00",
                    "classes": "",
                    "color": "#000000"
                  },
                  "text": "Auto",
                  "unselectedStyle": {
                    "classes": ""
                  },
                  "value": 1
                }
              ]
            },
            "type": "ia.input.multi-state-button"
          }
        ],
        "meta": {
          "name": "Pump HOA Cont"
        },
        "position": {
          "basis": "230px"
        },
        "props": {
          "direction": "column",
          "justify": "space-evenly"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "60px"
            },
            "props": {
              "style": {
                "color": "--neutral-100",
                "textAlign": "center"
              },
              "text": "Cond Water Temp"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "basis": "30px"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "numberFormat({[default]BMS/Blue/HVAC/AC2/Cntl/Cond_Water_Temp}, {[default]BMS/Blue/HVAC/AC2/Cntl/Cond_Water_Temp.FormatString}) + \u0027Â°F\u0027"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "backgroundColor": "--neutral-80",
                "borderColor": "--neutral-50",
                "borderStyle": "solid",
                "color": "--neutral-10",
                "fontWeight": "bold",
                "textAlign": "right"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Water Temp"
        },
        "position": {
          "basis": "89px"
        },
        "props": {
          "direction": "column",
          "justify": "space-evenly"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "60px"
            },
            "props": {
              "style": {
                "color": "--neutral-100",
                "textAlign": "center"
              },
              "text": "Status"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Status"
            },
            "position": {
              "basis": "30px"
            },
            "propConfig": {
              "props.style.backgroundColor": {
                "binding": {
                  "config": {
                    "path": "this.props.text"
                  },
                  "transforms": [
                    {
                      "code": "\tif value \u003d\u003d \u0027Off\u0027:\n\t\treturn \u0027#D5D5D5\u0027\n\telif value \u003d\u003d \u0027Running\u0027:\n\t\treturn \u0027#00FF00\u0027\n\telif value \u003d\u003d \u0027Run Fault\u0027:\n\t\treturn \u0027\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.style.classes": {
                "binding": {
                  "config": {
                    "path": "this.props.text"
                  },
                  "transforms": [
                    {
                      "code": "\tif value \u003d\u003d \u0027Run Fault\u0027:\n\t\treturn \u0027building-automation-demo/page/controls/framework/fault\u0027\n\telse:\n\t\treturn \u0027\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.style.color": {
                "binding": {
                  "config": {
                    "expression": "if({session.props.theme}\u003d\u0027hvac-dark\u0027 || {session.props.theme}\u003d\u0027hvac-dark-warm\u0027,\u0027--neutral-10\u0027,\u0027--neutral-100\u0027)"
                  },
                  "type": "expr"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "direct",
                    "tagPath": "[default]BMS/Blue/HVAC/AC2/Cond_Pump/Status"
                  },
                  "transforms": [
                    {
                      "code": "\tif value \u003d\u003d 0:\n\t\treturn \u0027Off\u0027\n\telif value \u003d\u003d 1:\n\t\treturn \u0027Running\u0027\n\telif value \u003d\u003d 2:\n\t\treturn \u0027Run Fault\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "tag"
                }
              }
            },
            "props": {
              "style": {
                "borderColor": "--neutral-50",
                "borderStyle": "solid",
                "fontWeight": "bold",
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Status"
        },
        "position": {
          "basis": "105px"
        },
        "props": {
          "direction": "column",
          "justify": "space-evenly"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "justify": "space-between",
      "style": {
        "overflow": "hidden",
        "paddingLeft": "15px",
        "paddingRight": "15px"
      }
    },
    "type": "ia.container.flex"
  }
}